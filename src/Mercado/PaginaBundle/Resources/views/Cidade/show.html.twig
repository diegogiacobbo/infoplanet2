{% extends "GerenciadorLoginBundle::layout.html.twig" %}

{% block title "Cidade" %}

{% block content %}
<h1>Cidade</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nome</th>
            <td>{{ entity.nome }}</td>
        </tr>
        <tr>
            <th>Estado</th>
            <td>{{ entity.estado }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('admin_cidade') }}">
            Back to the list
        </a>
    </li>
</ul>

{% endblock %}